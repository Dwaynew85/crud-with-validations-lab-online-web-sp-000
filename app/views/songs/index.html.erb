<h1>Songs</h1>

<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td>Title: <%= song.title %></td>
        <td>Artist: <%= song.artist_name %></td>
        <td>Genre: <%= song.genre %></td>
        <td>Released: <%= song.released %></td>
        <td>Release Year: <%= song.release_year %></td>
      </tr>
    <% end %>
  </tbody>
</table>
